{
    "plugins": [
        "react"
    ],
    "extends": [ "airbnb-base", "eslint:recommended", "plugin:react/recommended"],
    "rules": {
        "comma-dangle": 0,
        "react/jsx-uses-vars": 1,
        "react/display-name": 1,
        "no-unused-vars": "warn",
        "no-console": 1,
        "no-unexpected-multiline": "warn",
        "import/no-named-as-default": 0,
        "import/no-named-as-default-member": 0,
        "no-alert": "off",
        "react/prop-types": 0
    },
    "env": {
        "browser": true,
        "node": true
      },
    "settings": {
        "react": {
            "pragma": "React",
            "version": "15.6.1"
        }
    }
}